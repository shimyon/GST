    <script type="text/javascript">
        if (localStorage['Token']) {
            var GlobalExpirydate = new Date(localStorage['.expires']);
            var GlobalCurdate = new Date();
            if (GlobalExpirydate < GlobalCurdate) {
                alert('Session expired please login again.');
                location.replace("/Login");
            }
            if (localStorage['userRole'] == "Sale") {
                document.write("<style> .noSales { display: none !important; } </style>");
            }
            if (localStorage['userRole'] == "Manager") {
                document.write("<style> .noManager { display: none !important; } </style>");
            }
        } else {
            location.replace("/Login");
        }
    </script>
